<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.wslivestream" name="wsLivestream" version="2017.5.24" provider-name="WZS">
  <requires>
   <import addon="xbmc.python" version="2.19.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="main.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Livestream.com Live Streams Only without clutter, and the extra clicking!</summary>
    <description lang="en">Livestream live streams only provided by WZS based on plugin.video.livestream but without extra clicking.</description>
    <language>en</language>
    <platform>all</platform>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
    <forum>http://forum.kodi.tv/showthread.php?tid=230355</forum>
    <website>https://livestream.com/</website>
    <source>https://github.com/eracknaphobia/plugin.video.livestream</source>
  </extension>
</addon>

<addon
    id="plugin.video.wslivetv"
    version="3.1.9"
    name="wslivetv"
    provider-name="WZS">
    <requires>
        <import addon="xbmc.python"                 version="2.1.0" />
        <import addon="script.module.googleapi"     version="1.4.2" />
        <import addon="script.common.plugin.cache"  version="2.5.5" />
        <import addon="script.module.requests"      version="2.12.4"/>
    </requires>
    <extension
        point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Live Arabic Youtube Streams Plugin</summary>
        <description lang="en">This plugin retrieves the current live Arabic channels as well as some select english channels from Youtube. It saves the live TV channels DB locally and updates it on plugin start. The retrieved channels vary based on what channel is live at the time of plugin run.
Enjoy the live channels, and provide your feedback for continuous improvement
        </description>
        <disclaimer lang="en">channels may not be avialble all the time</disclaimer>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
        <news>Create categories, and context menu</news>
    </extension>
</addon>

<addon id="plugin.video.wsteledunet"
       name="WSTeledunet.com"
       version="3.2.0"
       provider-name="WSZ">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.beautifulsoup" version="3.0.8"/>
        <import addon="script.module.xbmcswift2" version="1.1.1" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Teledunet.com</summary>
        <description>Télé du Net is a free online broadcasting service based in Tunisia that is dedicated to online viewers that live outside the Arab world.

It broadcasts channels through the internet 24/7. The service is completely free to use and has a great compilation of channels to choose from.</description>
        <platform>all</platform>
    </extension>
</addon>

    <addon id="repository.wsrepo" name="WS Repo" version="0.1.0" provider-name="WZS">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="WS Repo">
            <info compressed="false">https://github.com/welwel2/wsrepo/tree/master/_repo/addons.xml/?raw=true_repo</info>
            <checksum>https://github.com/welwel2/wsrepo/tree/master/_repo/addons.xml.md5/?raw=true_repo</checksum>
            <datadir zip="true">https://github.com/welwel2/wsrepo/tree/master/_repo/?raw=true_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>This repo contains all the latest WS addons</summary>
            <description>WS addons are proven high quality addon[CR] Enjoy.</description>
            <platform>all</platform>
    </extension>
</addon>
</addons>
