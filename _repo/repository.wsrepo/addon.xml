<?xml version="1.0" encoding="UTF-8"?>
<addon
    id="repository.wsrepo"
    name="WS Repo"
    version="1.0.0"
    provider-name="WZS">
    <requires>
        <import
            addon="xbmc.addon"
            version="12.0.0"/>
    </requires>
    <extension
        point="xbmc.addon.repository"
        name="WS Repo">
        <info compressed="false">https://raw.githubusercontent.com/welwel2/wsrepo/master/_repo/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/welwel2/wsrepo/master/_repo/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/welwel2/wsrepo/master/_repo/</datadir>
        <hashes>false</hashes>
    </extension>
    <extension point="xbmc.addon.metadata">
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
        <summary>This repo contains all the latest WS addons</summary>
        <description>WS addons are proven high quality addon[CR] Enjoy.</description>
        <platform>all</platform>
    </extension>
</addon>